<h1>Editing Enrollment #<%= @enrollment.id %></h1>

<%= form_tag(enrollment_url(@enrollment), method: 'patch') do %>
  <div>
    <%= label_tag :student_id %><br />
    <%= number_field_tag :student_id, @enrollment.student_id %>
  </div>

  <div>
    <%= label_tag :section_id %><br />
    <%= number_field_tag :section_id, @enrollment.section_id %>
  </div>

  <div>
    <%= submit_tag "Update Enrollment" %>
  </div>
<% end %>
