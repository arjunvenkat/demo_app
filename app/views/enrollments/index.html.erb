<h1>Enrollments</h1>

<p><%= link_to 'Add a New Enrollment', new_enrollment_url %></p>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Section</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @enrollments.each do |enrollment| %>
    <tr>
      <td><%= enrollment.student_id %></td>
      <td><%= enrollment.section_id %></td>
      <td><%= link_to 'Show', enrollment_url(enrollment) %></td>
      <td><%= link_to 'Edit', edit_enrollment_url(enrollment) %></td>
      <td><%= link_to 'Destroy', enrollment_url(enrollment), method: 'delete', data: { confirm: "Do you really want to delete this enrollment?" } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

