<h1>New Enrollment</h1>

<%= form_tag(enrollments_url, method: 'post') do %>
  <div>
    <%= label_tag :student_id %><br />
    <%= number_field_tag :student_id %>
  </div>

  <div>
    <%= label_tag :section_id %><br />
    <%= number_field_tag :section_id %>
  </div>

  <div>
    <%= submit_tag "Create Enrollment" %>
  </div>
<% end %>
