<h1>Editing Student #<%= @student.id %></h1>

<%= form_tag(student_url(@student), method: 'patch') do %>
  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name, @student.name %>
  </div>

  <div>
    <%= label_tag :image %><br />
    <%= text_field_tag :image, @student.image %>
  </div>

  <div>
    <%= submit_tag "Update Student" %>
  </div>
<% end %>
